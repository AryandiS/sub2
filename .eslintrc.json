{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "operator-linebreak": "off",
    "quotes": "off",
    "import/no-extraneous-dependencies": 0,
    "no-underscore-dangle": 0,
    "linebreak-style": 0,
    "no-restricted-globals": 0,
    "no-use-before-define": 0,
    "func-names": 0,
    "import/extensions": "never"
  }
}
